    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.1",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuetify": "^3.9.4"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^24.0.0",
    "typescript": "~5.8.3",
