{
  "name": "frontend_axenta",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build --mode production",
    "build-check": "vue-tsc -b && vite build",
    "preview": "vite preview --host",
